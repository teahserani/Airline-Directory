bool equalsArray(int *numbers1,int *numbers2,int length) 
{
	boolean equals=false;
	for(int i=0; i<length; i++)
	{
		if(numbers1[i]==numbers2[i])
		{
			equals=true;
		}
		
	}
	return equals;
}
